@model IEnumerable<MovieWeb.Models.AppUser>

<h1>All Users</h1>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Username</th>
            <th>Full Name</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.FullName</td>
                <td>@user.Role</td>
            </tr>
        }
    </tbody>
</table>
